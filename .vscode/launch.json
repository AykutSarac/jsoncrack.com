{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run VSCode Extension (apps/vscode)",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/apps/vscode"
      ],
      "outFiles": [
        "${workspaceFolder}/apps/vscode/build/**/*.js"
      ],
      "preLaunchTask": "build vscode extension"
    }
  ]
}
